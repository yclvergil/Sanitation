<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>
    <!-- Application theme. -->

    <style name="progress_theme">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="product_radio_btn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">@dimen/text_size_2</item>
        <item name="android:drawablePadding">1dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="product_edit">
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/text_size_2</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:imeOptions">actionDone</item>
    </style>

    <style name="CustomTabPageIndicator" parent="TabPageIndicator">
        <item name="android:textAppearance">@style/CustomTabPageIndicator.Text</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:divider">@null</item>
        <item name="android:height">40dp</item>
    </style>

    <style name="TabPageIndicator">
        <item name="android:gravity">center</item>
        <item name="android:background">#ffffff</item>
        <item name="android:paddingLeft">22dip</item>
        <item name="android:paddingRight">22dip</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="normal_text" parent="@android:style/Widget">
        <item name="android:textColor">@color/dark_gray</item>
    </style>

    <!-- 第3方开始 -->
    <!-- dialog -->
    <style name="CommonDialog" parent="@android:style/Theme.Dialog">

        <!-- <item name="android:windowBackground">@drawable/smssdk_dialog_bg</item> -->
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- white dialog -->
    <style name="WhiteDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/dialog_white_bg</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- 第3方结束 -->


    <!-- indicator -->

    <style name="indicator">
        <item name="android:clickable">true</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="indicaotr_button">
        <item name="android:clickable">false</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:duplicateParentState">true</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="indicaotr_note" parent="@style/normal_text">
        <item name="android:clickable">false</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:visibility">invisible</item>
        <item name="android:digits">0123456789</item>
        <item name="android:layout_gravity">top|right</item>
        <item name="android:background">@drawable/ic_icon_solidcircle_orange</item>
    </style>

    <style name="indicaotr_text" parent="@style/normal_text">
        <item name="android:clickable">false</item>
        <item name="android:duplicateParentState">true</item>
